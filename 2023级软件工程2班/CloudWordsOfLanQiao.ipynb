   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from wordcloud import WordCloud\n",
    "\n",
    "# 1. 提取图中知识点 + 分配权重（基础=2，提高=3，进阶=4，书名核心=5）\n",
    "word_freq = {\n",
    "    "算法设计与可视化": 5,  # 书名核心\n",
    "    "编程基础": 2,\n",
    "    "搜索": 2,\n",
    "    "图论-基础": 2,\n",
    "    "图论-提高": 3,\n",
    "    "计算几何": 3,\n",
    "    "图论-进阶": 4,\n",
    "    "基础算法": 2,\n",
    "    "动态规划-基础": 2,\n",
    "    "动态规划-背包问题": 3,\n",
    "    "动态规划-提高": 3,\n",
    "    "字符串-提高": 3,\n",
    "    "字符串-进阶": 4,\n",
    "    "排序算法": 2,\n",
    "    "数学-基础": 2,\n",
    "    "数学-提高": 3,\n",
    "    "排列组合": 3,\n",
    "    "博弈论": 3,\n",
    "    "数学-进阶": 4,\n",
    "    "数据结构-基础": 2,\n",
    "    "高级数据结构": 3,\n",
    "    "数据结构-进阶": 4\n",
    "}\n",
    "\n",
    "# 2. 生成圆形mask（让词云呈现圆形）\n",
    "def create_circle_mask(size=500):\n",
    "    x, y = np.ogrid[:size, :size]\n",
    "    mask = (x - size/2)**2 + (y - size/2)**2 > (size/2)**2\n",
    "    mask = 255 * mask.astype(int)\n",
    "    return mask\n",
    "\n",
    "circle_mask = create_circle_mask(800)  # 圆形画布大小\n",
    "\n",
    "# 3. 配置词云（需替换为你的中文字体路径，比如SimHei.ttf）\n",
    "wc = WordCloud(\n",
    "    font_path="/usr/share/fonts/opentype/noto/NotoSerifCJK-Regular.ttc",  # Cloud Studio环境中的中文字体\n",
    "    background_color="white",\n",
    "    mask=circle_mask,  # 圆形形状\n",
    "    max_words=50,\n",
    "    max_font_size=100,\n",
    "    random_state=42\n",
    ")\n",
    "\n",
    "# 4. 生成词云\n",
    "wc.generate_from_frequencies(word_freq)\n",
    "\n",
    "# 5. 显示并保存\n",
    "plt.figure(figsize=(10, 10))\n",
    "plt.imshow(wc)\n",
    "plt.axis("off")\n",
    "plt.savefig("/workspace/2023级软件工程2班/算法设计与可视化_知识点词云.png", dpi=300)  # 保存为图片\n",
    "plt.show()"
   ]
  }
 ],
